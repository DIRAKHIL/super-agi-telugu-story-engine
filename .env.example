# Telugu Story Engine Environment Variables
# PRODUCTION-READY REAL AI SYSTEM - 100% open source OPERATIONAL!

# Environment
ENVIRONMENT=production
USE_GPU=true
LOG_LEVEL=INFO

# API Keys (optional)
OPENAI_API_KEY=your_openai_api_key
AI4BHARAT_API_KEY=your_ai4bharat_api_key

# Security
API_KEY=your_api_key
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_password
DASHBOARD_SECRET_KEY=your_secret_key

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/story_engine

# Redis
REDIS_URL=redis://localhost:6379/0

# Models
STORY_MODEL=local:./models/llama-3-8b-telugu-instruct.gguf

# PGAdmin
PGADMIN_EMAIL=admin@telugu-ai.org
PGADMIN_PASSWORD=admin